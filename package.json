{
  "name": "video-trim-cloud",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "start:cron": "ts-node src/modules/process-video/processVideo.schedule.ts",
    "start:conc": "concurrently \"npm run start\" \"npm run start:cron\"",
    "start:watch": "nodemon",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build": "rm -rf ./build && tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "1.0.20",
    "axios": "0.20.0",
    "concurrently": "5.3.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-fileupload": "1.2.0",
    "fluent-ffmpeg": "2.1.2",
    "node-cron": "2.0.3",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.7",
    "@types/express": "4.17.8",
    "@types/express-fileupload": ".1.4",
    "@types/node": "14.6.4",
    "nodemon": "2.0.4",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run build && npm start",
    "ext": "ts"
  }
}
